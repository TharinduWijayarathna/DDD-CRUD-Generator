{
    "name": "tharindu/ddd_generator",
    "description": "A Laravel package to generate facades and services.",
    "type": "library",
    "license": "MIT",
    "require": {
        "php": ">=8.1",
        "illuminate/support": "^10.0|^11.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "Tharindu\\DDDGenerator\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Tharindu\\DDDGenerator\\DomainServiceProvider"
            ]
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}